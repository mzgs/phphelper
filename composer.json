{
    "name": "mzgs/phphelper",
    "description": "A comprehensive PHP helper library with database, file, login, and utility helpers",
    "type": "library",
    "require": {
        "php": "^8.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "PHP Helper",
            "email": "info@phphelper.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}